# Auth0 Configuration for Local Development
# Copy this file to .env.local and update with your Auth0 credentials

# Your Auth0 domain (found in Auth0 Dashboard > Applications > Your App > Settings)
REACT_APP_AUTH0_DOMAIN=your-tenant.auth0.com

# Your Auth0 Client ID (found in Auth0 Dashboard > Applications > Your App > Settings)
REACT_APP_AUTH0_CLIENT_ID=your-client-id-here

# Bypass Authentication (for development/testing only)
# Set to 'true' to disable authentication requirement
# WARNING: Never use this in production!
REACT_APP_BYPASS_AUTH=false